<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/branches">

    <fragment
        android:id="@+id/branches"
        android:name="com.example.ownerapp.Fragments.Home"
        android:label="Home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_branches_to_specific2"
            app:destination="@id/specific" />
    </fragment>
    <fragment
        android:id="@+id/settings"
        android:name="com.example.ownerapp.Fragments.Settings"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" >
        <action
            android:id="@+id/action_settings_to_specific2"
            app:destination="@id/specific" />
    </fragment>
    <fragment
        android:id="@+id/specific"
        android:name="com.example.ownerapp.Fragments.Specific"
        android:label="Specific"
        tools:layout="@layout/fragment_specific">
        <action
            android:id="@+id/action_specific2_to_settings"
            app:destination="@id/settings" />
        <action
            android:id="@+id/action_specific2_to_branches"
            app:destination="@id/branches" />
    </fragment>
</navigation>